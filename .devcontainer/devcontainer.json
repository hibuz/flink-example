{
    "name": "Java",
    "dockerComposeFile": "docker-compose.yml",
    "service": "flink-dev",
    "workspaceFolder": "/home/hadoop/flink-example",
    "features": {
      "ghcr.io/devcontainers/features/docker-outside-of-docker:1": {}
    },
    "postCreateCommand": "java -version",
    "customizations": {
      "vscode": {
        "extensions": [
          "vscjava.vscode-java-pack",
          "eamodio.gitlens"
        ],
        "settings": {
          "locale": "en"
        }
      }
    }
}