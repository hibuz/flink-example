{
    "name": "Java",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "flink-dev",
    "workspaceFolder": "/home/hadoop/flink-example",
    "remoteEnv": {
      "DOCKER_API_VERSION": "1.43"
    },
    "features": {
      "ghcr.io/devcontainers/features/docker-outside-of-docker:1": {}
    },
    "postCreateCommand": "git clone https://github.com/hibuz/kafka-all-in-one.git /workspaces/kafka-all-in-one",
    "customizations": {
      "vscode": {
        "extensions": [
          "vscjava.vscode-java-pack",
          "eamodio.gitlens"
        ],
        "settings": {
          "locale": "en"
        }
      }
    },
    "forwardPorts": [
        9870,
        8088,
        19888,
        8090,
        8091,
        18080,
        4040,
        10002,
        8081,
        9090,
        8181
    ]
}